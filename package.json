			"id": "sass-indented",
			"aliases": ["Sass (Indented)", "sass-indented"],
			"extensions": [".sass"],
			"configuration": "./sass-indented.configuration.json"
		}],
		"grammars": [{
			"language": "sass-indented",
			"scopeName": "source.sass",
			"path": "./syntaxes/sass.tmLanguage"
		}],
		"snippets": [{
			"language": "sass-indented",
			"path": "./snippets/sass-indented.json"
		}]
