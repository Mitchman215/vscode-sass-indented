{
  "name": "sass-indented",
  "displayName": "Sass",
  "description": "Indented Sass syntax Highlighting, Autocomplete & Formatter",
  "version": "1.6.0",
  "icon": "resources/images/sass-logo-seal.png",
  "publisher": "syler",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRealSyler/vscode-sass-indented"
  },
  "bugs": "https://github.com/TheRealSyler/vscode-sass-indented/issues/new?assignees=TheRealSyler&labels=bug&template=bug_report.md&title=",
  "engines": {
    "vscode": "^1.30.0"
  },
  "activationEvents": [
    "onLanguage:sass",
    "onView:snippets"
  ],
  "main": "./out/extension",
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Sass (Indented)",
      "properties": {
        "sass.disableAutoIndent": {
          "type": "boolean",
          "default": false,
          "description": "Stop the extension from automatically indenting when pressing Enter"
        },
        "sass.disableUnitCompletion": {
          "type": "boolean",
          "default": true,
          "description": "adds units to the intellisense completions if false."
        },
        "sass.format.enabled": {
          "type": "boolean",
          "default": false,
          "description": "disables the sass formatter."
        },
        "sass.format.deleteWhitespace": {
          "type": "boolean",
          "default": true,
          "description": "removes trailing whitespace."
        },
        "sass.format.deleteEmptyRows": {
          "type": "boolean",
          "default": true,
          "description": "removes empty rows."
        },
        "sass.format.deleteCompact": {
          "type": "boolean",
          "default": true,
          "description": "also removes empty rows that are near a property."
        },
        "sass.format.debug": {
          "type": "boolean",
          "default": false,
          "description": "enables debug mode."
        },
        "sass.snippets.path": {
          "type": "string",
          "default": "none",
          "description": "path for the global snippets file."
        },
        "sass.andStared": {
          "type": "array",
          "default": [
            "active",
            "focus",
            "hover",
            "nth-child"
          ],
          "description": "items in this array will be at the top of the completion list (only for items that show after the & sign)"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sass",
          "title": "Sass",
          "icon": "resources/images/Sass_Logo_Color.svg"
        }
      ]
    },
    "views": {
      "sass": [
        {
          "id": "snippets",
          "name": "Snippets"
        }
      ]
    },
    "commands": [
      {
        "command": "sass.tree.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        },
        "category": "Sass"
      },
      {
        "command": "sass.tree.addFromSelection",
        "title": "Add Snippet From Selection",
        "category": "Sass"
      },
      {
        "command": "sass.tree.delete",
        "title": "Delete",
        "category": "Sass"
      },
      {
        "command": "sass.tree.edit",
        "title": "Edit",
        "category": "Sass"
      },
      {
        "command": "sass.tree.addFolder",
        "title": "Add Folder",
        "category": "Sass"
      },
      {
        "command": "sass.tree.moveUp",
        "title": "⯅ Move Up",
        "category": "Sass"
      },
      {
        "command": "sass.tree.moveDown",
        "title": "⯆ Move Down",
        "category": "Sass"
      },
      {
        "command": "sass.tree.copy",
        "title": "Copy",
        "category": "Sass"
      },
      {
        "command": "sass.tree.paste",
        "title": "Paste",
        "category": "Sass"
      },
      {
        "command": "sass.tree.insert",
        "title": "Insert",
        "category": "Sass"
      },
      {
        "command": "sass.tree.openFile",
        "title": "Open File",
        "category": "Sass"
      }
    ],
    "keybindings": [
      {
        "command": "sass.tree.addFromSelection",
        "key": "shift+alt+d",
        "mac": "shift+alt+d",
        "when": "resourceExtname == .sass && editorHasSelection"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "sass.tree.refreshEntry",
          "when": "false"
        },
        {
          "command": "sass.tree.addFromSelection",
          "when": "false"
        },
        {
          "command": "sass.tree.delete",
          "when": "false"
        },
        {
          "command": "sass.tree.edit",
          "when": "false"
        },
        {
          "command": "sass.tree.addFolder",
          "when": "false"
        },
        {
          "command": "sass.tree.moveUp",
          "when": "false"
        },
        {
          "command": "sass.tree.moveDown",
          "when": "false"
        },
        {
          "command": "sass.tree.copy",
          "when": "false"
        },
        {
          "command": "sass.tree.paste",
          "when": "false"
        },
        {
          "command": "sass.tree.insert",
          "when": "false"
        },
        {
          "command": "sass.tree.openFile",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "sass.tree.refreshEntry",
          "when": "view == snippets",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "sass.tree.addFromSelection",
          "group": "navigation",
          "when": "viewItem != snippet"
        },
        {
          "command": "sass.tree.insert",
          "group": "navigation",
          "when": "viewItem == snippet"
        },
        {
          "command": "sass.tree.delete",
          "group": "2_edit",
          "when": "viewItem != root"
        },
        {
          "command": "sass.tree.edit",
          "group": "2_edit",
          "when": "viewItem != root"
        },
        {
          "command": "sass.tree.addFolder",
          "group": "2_edit",
          "when": "viewItem != snippet"
        },
        {
          "command": "sass.tree.moveUp",
          "group": "3_move",
          "when": "viewItem != root"
        },
        {
          "command": "sass.tree.moveDown",
          "group": "3_move",
          "when": "viewItem != root"
        },
        {
          "command": "sass.tree.copy",
          "group": "1_copyPaste",
          "when": "viewItem != root"
        },
        {
          "command": "sass.tree.paste",
          "group": "1_copyPaste",
          "when": "viewItem != snippet"
        },
        {
          "command": "sass.tree.openFile",
          "group": "navigation",
          "when": "viewItem == root"
        }
      ],
      "editor/context": [
        {
          "command": "sass.tree.addFromSelection",
          "group": "navigation",
          "when": "resourceExtname == .sass"
        }
      ]
    },
    "languages": [
      {
        "id": "sass",
        "aliases": [
          "Sass",
          "sass-indented"
        ],
        "extensions": [
          ".sass",
          ".sass.erb"
        ],
        "configuration": "./sass.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sass",
        "scopeName": "source.sass",
        "path": "./syntaxes/sass.language.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'"
  },
  "devDependencies": {
    "typescript": "^3.2.2",
    "vscode": "^1.1.26",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25"
  }
}
