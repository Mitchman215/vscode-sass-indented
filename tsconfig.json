{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ES2019"],
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "outDir": "out",
    "resolveJsonModule": true,
    "rootDir": "scripts"
  },
  "include": ["scripts"],
  "exclude": ["node_modules"]
}
